{
  "name": "mcp-probe-kit",
  "version": "2.2.0",
  "description": "AI-Powered Development Toolkit - MCP Server with 39 practical tools covering code quality, development efficiency, project management, and UI/UX design. Features: Structured Output, Workflow Orchestration, UI/UX Pro Max, and Requirements Interview.",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "mcp-probe-kit": "build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsc && node build/index.js",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:server": "npm run build && node test-server.js",
    "inspector": "npx @modelcontextprotocol/inspector node build/index.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "sync-ui-data": "tsx scripts/sync-ui-data.ts",
    "sync-ui-data:verbose": "tsx scripts/sync-ui-data.ts --verbose",
    "prebuild": "npm run sync-ui-data"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cursor",
    "ai-tools",
    "development-tools",
    "code-quality",
    "code-review",
    "refactor",
    "debugging",
    "sql-generator",
    "ui-generator",
    "react",
    "vue",
    "typescript",
    "git-tools",
    "ai-assistant"
  ],
  "author": {
    "name": "小墨 (Kyle)",
    "url": "https://www.bytezonex.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mybolide/mcp-probe-kit.git"
  },
  "bugs": {
    "url": "https://github.com/mybolide/mcp-probe-kit/issues"
  },
  "homepage": "https://mcp-probe-kit.bytezonex.com",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "build",
    "docs",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "csv-parse": "^6.1.0",
    "tar": "^7.5.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/tar": "^6.1.13",
    "@vitest/ui": "^4.0.18",
    "fast-check": "^4.5.3",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
